struct Person {
	age: i32,
	score: i32
}

fn set_by_ptr(mut p: & i32, v: i32) {
	@p = v
}

fn main() -> i32 {
	x = 10
	mut ptr = &x
	print(@ptr)
	@ptr = 20
	print(x)
	arr = [1, 2, 3, 4, 5]
	p = &arr[0]
	second = @(p + 1)
	print(second)
	person= Person {
		age: 18,
		score: 95
	}
	mut age_ptr = &person.age
	print(@age_ptr)
	@age_ptr = 21
	print(person.age)
	set_by_ptr(&person.score, 100)
	print(person.score)
	return 0
}
